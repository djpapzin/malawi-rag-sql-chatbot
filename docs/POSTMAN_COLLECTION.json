{
  "info": {
    "name": "Malawi Projects Chatbot API",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Health Check",
      "request": {
        "method": "GET",
        "header": [],
        "url": {
          "raw": "{{base_url}}/health",
          "host": ["{{base_url}}"],
          "path": ["health"]
        }
      }
    },
    {
      "name": "Submit Query",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n    \"message\": \"List education projects\",\n    \"source_lang\": \"english\",\n    \"page\": 1,\n    \"page_size\": 5\n}"
        },
        "url": {
          "raw": "{{base_url}}/query",
          "host": ["{{base_url}}"],
          "path": ["query"]
        }
      }
    }
  ],
  "variable": [
    {
      "key": "base_url",
      "value": "http://localhost:8000/api/rag-sql-chatbot"
    }
  ]
}
